    (function() {
        var $ = window.optimizely.get('jquery');
        var tourInspectScrollHandler = debounce(fireEventOnInspectSectionViewed, 50);
        $(window).scroll(tourInspectScrollHandler);

        function fireEventOnInspectSectionViewed() {
            var $inspectSection = $('.tour-inspect .tour-content h2');
            if (isScrolledIntoView($inspectSection)) {
                fireEvent();
                $(window).unbind('scroll', tourInspectScrollHandler);
